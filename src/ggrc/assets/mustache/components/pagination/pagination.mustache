<div class="ggrc-pagination">
  <div class="count">
    <div class="dropdown count">
      <h6 class="dropdown-toggle"
          data-toggle="dropdown">{{paginationInfo paging.current paging.page_size paging.total}}</h6>
      <div class="dropdown-menu">
        <span>Items per page:</span>
        <div class="btn-group">
          {{#paging.page_size_select}}
            <button can-click="changePageSize" class="btn {{#if_equals paging.page_size .}}active{{/if_equals}}"
                      data-size="{{.}}">{{.}}</button>
          {{/paging.page_size_select}}
        </div>
      </div>
    </div>
  </div>
  <ul class="pagination pull-right">
    <li class="{{#if_equals paging.current 1}}disabled{{/if_equals}}">
      <a href="javascript://" can-click="firstPage">
        <i class="fa fa-angle-double-left" aria-hidden="true"></i>
      </a>
    </li>
    <li class="{{#if_equals paging.current 1}}disabled{{/if_equals}}">
      <a href="javascript://" can-click="prevPage">
        <i class="fa fa-angle-left" aria-hidden="true"></i>
      </a>
    </li>
    <li>
      <input type="text" can-change="setCurrentPage" class="input"
               placeholder="{{paginationPlaceholder paging.current paging.count}}">
    </li>
    <li class="{{#if_equals paging.current paging.count}}disabled{{/if_equals}}">
      <a href="javascript://" can-click="nextPage">
        <i class="fa fa-angle-right" aria-hidden="true"></i>
      </a>
    </li>
    <li class="{{#if_equals paging.current paging.count}}disabled{{/if_equals}}">
      <a href="javascript://" can-click="lastPage" class="">
        <i class="fa fa-angle-double-right" aria-hidden="true"></i>
      </a>
    </li>
  </ul>
  <div class="clearfix"></div>
</div>
